__lua__
--[[
todo:

*create a way to save the button layouts in any level

*create more levels

*update the bot to be able to jump and climb stairs and such
  > get bot climbing working

*maybe update bot code to let multiple enemys be on screen at once

*actually create the story for this game

*design the character for the game

*actually create interesting bot desgins

]]
-->8
--all code
function _init()
  #include .enemy.p8
  #include random.p8
  #include .movement.p8
  #include background.p8
  cartdata("dc_capstone")
  menuitem(1,"save",function() save() end)
  menuitem(2,"load",function() lload() end)
  menuitem(3,"main menu",function() load("main-menu.p8") end)
  menuitem(4,"debug file on/off",function() debug_any() end)
  menuitem(5,"reset save data",function() r_save() end)
  block = {x = -8, y = -8, w = 8, h = 8, sp = 78}
  special_pickup_init(96, 112, 51, 51)
  bg_init()
  door_init(112, 112)
  botinit()
  player_init()
  r_save(false)
  td_init()
  stored_combo = {}
  lvl1_buttons={
    {x = 352,y = 112, sp = 6, act = ".boss-room.p8", p = false, sfx = 0},
    {x = 200, y = 112, sp = 6, p = false, sfx = 0}, --opens end trapdoor -- 2
    {x = 88, y = 40, sp = 6, p = false, sfx = 0}, --opens trapdoors at 120, 80 -- 3
    {x = 288, y = 112, sp = 6, p = false, sfx = 0}, --opens the trapdoors att 96, 24 -- 4
    {x = 200, y = 88, sp = 6, p = false, sfx = 0}, -- opens the trapdoor at 160, 96 -- 5
    --buttons for combo lock
    {x = 152, y = 72, sp = 6, p = false, sfx = 0, num = 1},--third button in seq
    {x = 176, y = 72, sp = 6, p = false, sfx = 0, num = 2},--first button in seq
    {x = 200, y = 72, sp = 6, p = false, sfx = 0, num = 3},--second button in seq
    --buttons for door
    {x = 144, y = 112, sp = 41, p = false, sfx = 0},--9
    {x = 224, y = 32, sp = 43, p = false, sfx = 0},--10
    {x = 96, y = 88, sp = 6, p = false, sfx = 0}}--11
  dset(12,1)
  if dget(13,true) then
    dset(13,false)
    lload()
  end
end

--update and draw
function _update()
  bg_update()
  if lvl1_buttons[1].sfx > 0 then
    save()
    r_save(false)
    dset(15, 2)
    load(lvl1_buttons[1].act)
  end
  reset_combo = combo_lock_update(stored_combo, lvl1_buttons, {2, 3, 1}, 3, 200, 40, 8, 8)
  for i = 6, 8 do
    --for the buttons we want to check
    if lvl1_buttons[i].p then
      --if the button 6 < i < 8 is pressed
      if #stored_combo == 0 then
        --if the combo length is 0 then add it to the combo list without any other restrictions
        add(stored_combo, lvl1_buttons[i].num)
      else
        if stored_combo[#stored_combo] != lvl1_buttons[i].num then
          --if the most recent number in the list is NOT the same as the number in the button list at 6 < i < 8
          --then add it into the list as well
          add(stored_combo, lvl1_buttons[i].num)
        end--if
      end--else
    end--if
  end--for
  if reset_combo then
    --reset the buttons when you get a wrong combo and play a sound effect
    stored_combo = {}
    for i = 6, 8 do
      lvl1_buttons[i].p = false
      lvl1_buttons[i].sfx = 0
      lvl1_buttons[i].sp = 6
    end
  end
  td_update(1)
  td_grav()
  door_update(lvl1_buttons, {9,10,11})
  player_update()
  special_pickup_update()
  check_block_collision(block)
  ladder()
  stairs()
  td_update()
  manage_health()
  btn_update(lvl1_buttons, player)
  player_animate()
 	cam_update(0,244,0)
end

function _draw()
  cls()
  map(64 + (player.x - 8)/17, 0, cam_x, cam_y)
  map(0,0)
  btn_draw(lvl1_buttons)
  td_draw()
  special_pickup_draw()
  door_draw()
  draw_block(block)
  spr(player.sp, player.x, player.y , 1, 1, player.flp)
  spr(atk_spr.spr, atk_spr.x, atk_spr.y, 1, 1, atk_spr.flp)
  if proj.dx != 0 then
    spr(24, proj.x, proj.y, 1, 1, proj.flp)
  end
  draw_health()
end

